    "dotenv": "^10.0.0",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.9.3",
    "nodemailer": "^7.0.10",
    "nodemailer-express-handlebars": "^6.1.2",
    "stripe": "^15.8.0",
    "swagger-jsdoc": "^6.2.8",
